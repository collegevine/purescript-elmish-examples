{
    "scripts": {
        "postinstall": "spago install",
        "bundle-ps": "spago bundle-module --to ./output/bundle.js",
        "build": "npm run bundle-ps && parcel build index.html",
        "start": "npm run bundle-ps && concurrently \"spago bundle-module --to ./output/bundle.js --watch\" \"parcel index.html\""
    },
    "devDependencies": {
        "concurrently": "^4.1.0",
        "lodash": "^4.17.13",
        "parcel": "^1.12.3",
        "purescript": "^0.13.4",
        "purescript-psa": "0.7.3",
        "react-bootstrap": "^1.0.0-beta.8",
        "request": "^2.88.0",
        "spago": "^0.9.0"
    },
    "dependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    }
}
