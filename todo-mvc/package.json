    "lodash": "^4.17.21",
    "mixin-deep": ">=1.3.2",
    "esbuild": "^0.13.9",
    "purescript": "^0.15.4",
    "purescript-psa": "^0.8.2",
    "request": "^2.88.0",
    "safer-eval": "^1.3.6",
    "set-value": "^4.0.1",
    "spago": "^0.20.9"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "todomvc-app-css": "^2.0.0",
    "todomvc-common": "^1.0.0"
  }
